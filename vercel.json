{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".vitepress/dist",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": ".vitepress/dist/$1",
      "status": 200
    },
    {
      "src": "/",
      "dest": ".vitepress/dist/index.html",
      "status": 200
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "outputDirectory": ".vitepress/dist"
}