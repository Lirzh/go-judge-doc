{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".vitepress/dist",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": ".vitepress/dist/assets/$1",
      "status": 200
    },
    {
      "src": "^/cn/(.*)",
      "dest": ".vitepress/dist/cn/$1",
      "status": 200
    },
    {
      "src": "^/(.*)\.html",
      "dest": ".vitepress/dist/$1.html",
      "status": 200
    },
    {
      "src": "^/(.*)",
      "dest": ".vitepress/dist/$1.html",
      "status": 200
    },
    {
      "src": "^/$",
      "dest": ".vitepress/dist/index.html",
      "status": 200
    },
    {
      "src": ".*",
      "dest": ".vitepress/dist/404.html",
      "status": 404
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "outputDirectory": ".vitepress/dist"
}